{
    "name": "ykuadrat/laravel-datatable-builder",
    "description": "Datatable builder based on data from yajra/datatable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yasser",
            "email": "yassersultan1999@gmail.com"
        }
    ],
    "require": {
        "yajra/laravel-datatables-oracle": "9.*"
    },
    "autoload": {
        "psr-4": {
            "YKuadrat\\DatatableBuilder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "laravel-6": "1.0.0"
        },
        "laravel": {
            "providers": [
                "YKuadrat\\DatatableBuilder\\DatatableBuilderProvider"
            ],
            "aliases": {
                "DatatableBuilderHelper": "YKuadrat\\DatatableBuilder\\Helpers\\DatatableBuilderHelper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
